{
    // 配件重量，单位 kg。影响持枪者的移速
    "weight": 0.15,
    // 对瞄准时间的影响，加数，单位为秒，可为负数
    "ads_addend": 0.1,
    // 对精准度的影响，加数，可为负数。负数代表精准度增加，正数表示精准度降低。
    "inaccuracy_addend": -0.25,
    "silence": {
      // 声音传播的范围，如果默认是 50 格，这里填入 -10，那么消音后传播距离为 40 格
      "distance_addend": -10,
      // 是否使用专门的消音后的音源
      "use_silence_sound": true
    },
    // 对枪械后坐力的影响。
    "recoil_modifier": {
      // 竖直方向偏转削减的大小，范围-1~0
      "pitch": -0.35,
      // 水平方向偏转削减程度的大小，范围-1~0
      "yaw": 0
    }
  }