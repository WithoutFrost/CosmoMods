{
  // 默认的模型，在包目录下的 models 文件夹中寻找，不可为空
  "model": "eos:gun/wa2000_snake_kiss_geo",
  // 默认材质，不可为空
  "texture": "eos:gun/uv/wa2000_snake_kiss",
  // 低模
  "lod": {
    "model": "eos:gun/lod/wa2000_snake_kiss",
    "texture": "eos:gun/lod/wa2000_snake_kiss"
  },
  // GUI/HUD 界面的 2D 材质，在包目录下的 textures 文件夹中寻找，可为空，若为空，则对应位置不会渲染
  "hud": "eos:gun/hud/wa2000_snake_kiss",
  // GUI/HUD 界面的 2D 材质，在包目录下的 textures 文件夹中寻找，可为空
